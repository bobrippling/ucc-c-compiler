#!/bin/sh

. "$(dirname "$0")/common.sh"

verbose=no
if test "$1" = -v
then
	verbose=yes
	shift
fi

if test $# -ne 1
then
	echo "Usage: $0 [-v] path/to/file" >&2
	exit 1
fi

set -e

if test $verbose = yes
then set -x
fi

$UCC -g -fno-debug-emit-compdir -S -o - "$1" \
	| awk '/section_begin.*dbg/,/section_end/' \
	> "$TEST_TMP"

diff -u "$TEST_TMP" "$1".debug
